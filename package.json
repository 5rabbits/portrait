{
  "name": "portrait",
  "version": "0.4.0",
  "description": "Base design for all 5Rabbits products",
  "keywords": [
    "commonjs",
    "es6",
    "library",
    "starter",
    "umd",
    "universal",
    "webpack"
  ],
  "homepage": "https://bitbucket.org/lemontech/ux",
  "bugs": "https://bitbucket.org/lemontech/ux/issues",
  "license": "MIT",
  "author": "5rabbits <5rabbits@lemontech.com> (http://5rabbits.com/)",
  "contributors": [
    "Cristian Ferrari <cferrari@lemontech.cl>",
    "Daniel Ochoa <dochoa@lemontech.cl>",
    "Rodrigo DÃ­az <rdiaz@lemontech.cl>"
  ],
  "files": [
    "lib"
  ],
  "main": "lib/portrait.js",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/lemontech/ux"
  },
  "scripts": {
    "profile": "NODE_ENV=production webpack --progress --config webpack/webpack.config.js --profile --json > webpack.stats.json",
    "build": "NODE_ENV=production webpack --progress --config webpack/webpack.config.js",
    "build-watch": "NODE_ENV=production webpack --progress --config webpack/webpack.config.js --watch",
    "dev": "NODE_ENV=development webpack-dev-server --config webpack/webpack.config.js --hot --inline --content-base lib/docs",
    "lint": "eslint --color --ext .js src example",
    "prepublish": "npm run build",
    "test": "NODE_ENV=test jest",
    "test-watch": "NODE_ENV=test jest --watch",
    "preversion": "npm run lint && SINGLE_RUN=true npm test"
  },
  "dependencies": {
    "bootstrap-sass": "3.3.7",
    "classnames": "^2.2.5",
    "font-awesome": "^4.7.0",
    "jquery": "^3.1.1",
    "uncontrollable": "4.1.0"
  },
  "devDependencies": {
    "babel": "6.3.13",
    "babel-core": "6.1.18",
    "babel-eslint": "7.1.1",
    "babel-jest": "20.0.3",
    "babel-loader": "6.1.0",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "clean-webpack-plugin": "0.1.14",
    "component-playground": "^1.3.2",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "^0.26.1",
    "enzyme": "2.6.0",
    "enzyme-to-json": "1.5.1",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "13.0.0",
    "eslint-loader": "1.6.1",
    "eslint-plugin-import": "^2.1.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.6.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.28.0",
    "identity-obj-proxy": "3.0.0",
    "imports-loader": "0.7.0",
    "jest": "20.0.3",
    "jest-directory-named-resolver": "jsonnull/jest-directory-named-resolver",
    "json-loader": "0.5.4",
    "lodash": "^4.17.4",
    "markdown-it": "^8.3.1",
    "node-sass": "^4.0.0",
    "raw-loader": "^0.5.1",
    "react": "^15.4.1",
    "react-addons-test-utils": "15.4.1",
    "react-docgen": "^2.13.0",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.2",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "1.12.9",
    "webpack-config": "6.2.1",
    "webpack-dev-server": "^1.16.2",
    "yaml-loader": "^0.4.0"
  },
  "peerDependencies": {
    "react": "^15.4.1"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "jest": {
    "testRegex": "-test.js$",
    "resolver": "jest-directory-named-resolver",
    "moduleDirectories": [
      "src",
      "src/components",
      "src/styles",
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(css|scss|sass)$": "identity-obj-proxy"
    },
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "html",
      "text-summary"
    ]
  }
}
